  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "offline": "sls offline start --reloadHandler --stage local",
    "watch": "tsc -w",
    "codegen:public": "graphql-codegen --config src/api/publicApi/codegen.yml",
    "codegen:restaurant": "graphql-codegen --config src/api/restaurantApi/codegen.yml",
    "codegen:admin": "graphql-codegen --config src/api/adminApi/codegen.yml",
    "codegen": "npm run codegen:public; npm run codegen:admin",
    "sequelize:migrate": "source .env.$STAGE && npx sequelize-cli db:migrate --url $SEQUELIZE_URL",
    "sequelize:migrate:revert": "source .env.$STAGE && npx sequelize-cli db:migrate:undo --url $SEQUELIZE_URL",
    "prettier": "prettier --write src/**/*.ts",
    "lint": "eslint './src' './serverless.config' './libs' --ext .ts",
    "lint:strict": "eslint './src' './serverless.config' './libs' --ext .ts --max-warnings=0"
  },
